<p id="notice"><%= notice %></p>

<h1>Data Visualizations</h1>

<table>
  <thead>
    <tr>
      <th>Cx</th>
      <th>Cy</th>
      <th>R</th>
      <th>Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_visualizations.each do |data_visualization| %>
      <tr>
        <td><%= data_visualization.cx %></td>
        <td><%= data_visualization.cy %></td>
        <td><%= data_visualization.r %></td>
        <td><%= data_visualization.id %></td>
        <td><%= link_to 'Show', data_visualization %></td>
        <td><%= link_to 'Edit', edit_data_visualization_path(data_visualization) %></td>
        <td><%= link_to 'Destroy', data_visualization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Visualization', new_data_visualization_path %>

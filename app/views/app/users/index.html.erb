<p id="notice"><%= notice %></p>

<h1>users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
      
        <td><%= link_to 'Edit', edit_app_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

